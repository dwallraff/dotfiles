### My stuffs
Host dreamhost
	HostName randolph.dreamhost.com
	User dwallraff
	IdentityFile ~/.ssh/generic.key
	ServerAliveInterval 60

Host jumpbox
	HostName 35.198.223.120
	User dwallraff
	IdentityFile ~/.ssh/jumpbox.key
	ServerAliveInterval 60

Host *.amazonaws.com
	User ubuntu
	IdentityFile ~/.ssh/aws-dwallraff-pcf.key
	ServerAliveInterval 60
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET
	ForwardAgent yes

Host github.com
	User dwallraff
	IdentityFile ~/.ssh/github.key
	ServerAliveInterval 60
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET
	ForwardAgent yes

Host *
	ServerAliveInterval 60
    IdentityFile ~/.ssh/generic.key
    ServerAliveInterval 60
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET



